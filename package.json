    "test:pumpfun": "ts-node scripts/test-pumpfun-integration.ts",
    "setup:pumpfun": "ts-node scripts/setup-pumpfun-files.ts",
    "grpc:start": "node dist/grpc/grpc-stream-app.js",
    "grpc:dev": "ts-node src/grpc/grpc-stream-app.ts",
